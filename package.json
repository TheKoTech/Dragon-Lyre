		"electronmon": "^2.0.2",
		"cross-env": "^7.0.3",
		"electron-builder": "^23.6.0",
		"wait-on": "^6.0.1",
		"concurrently": "^7.6.0"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
